<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
int main()
{
    int a,b,c,d;
    scanf("%d %d %d",&amp;a,&amp;b,&amp;c);
    if(a&gt;b&amp;&amp;a&gt;c)
        printf("%d",a);
    else
    {
        if(b&gt;a&amp;&amp;b&gt;c)
        printf("%d",b);
        else
        printf("%d",c);
    }
    return 0;
}
