#include &lt;stdlib.h&gt;
int main()
{
    int a,b,c,d;
    scanf("%d %d %d",&amp;a,&amp;b,&amp;c);
    if(a&gt;b)
        d=a;
        else
        d=b;
    if(d&gt;c)
        printf("%d",d);
        else
        printf("%d",c);
    return 0;
}
