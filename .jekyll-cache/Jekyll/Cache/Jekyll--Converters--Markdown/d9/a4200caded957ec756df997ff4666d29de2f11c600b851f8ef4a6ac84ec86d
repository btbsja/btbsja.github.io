#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
int main()
{
    int h,a[10],i,sum;
    for(i=0;i&lt;=9;i++)
        scanf("%d",&amp;a[i]);
        scanf("%d",&amp;h);
    sum=0;
    for(i=0;i&lt;=9;i++)
    {
        if(h&gt;=a[i])
        sum++;
    }
    printf("%d",sum);
    system("pause");
    return 0;
}
