<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>//C033æ‹¼æ¥æ³•
#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
int main()
{
    int i,j,k;
    for(i=1;i&lt;=9;i++)
        for(j=0;j&lt;=9;j++)
            for(k=0;k&lt;=9;k++)
                if(i*100+j*10+k==i*i*i+j*j*j+k*k*k)
                    printf("%d\n",i*100+j*10+k);
    system("pause");
    return 0;
}


//C034
#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
int main()
{
    int a,b,c,x;
    for(x=100;x&lt;=999;x++)
    {
        a=x/100;
        b=x/10%10;
        c=x%10;
        if(a*a*a+b*b*b+c*c*c==x)
        printf("%d\n",x);
    }
    system("pause");
    return 0;
}
