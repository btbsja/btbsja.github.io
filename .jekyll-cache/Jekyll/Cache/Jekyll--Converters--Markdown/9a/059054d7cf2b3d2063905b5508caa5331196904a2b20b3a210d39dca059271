#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
#include &lt;string.h&gt;
int main()
{
    char a[101],b[101];
    gets(a);
    gets(b);
    if(strcmp(a,b)&lt;=0)
    {
        puts(a);
        puts(b);
    }
    else
    {
        puts(b);
        puts(a);
    }
    system("pause");
    return 0;
}
